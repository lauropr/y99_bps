{
  "_version": "1.28.0",
  "sap.app": {
    "id": "y99.bps",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.8.4",
      "toolsId": "86d2f85d-b65f-4df2-af5d-c78e4a357f40"
    },
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata/sap/Y99_BP_SRV/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.84.33",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "y99.bps.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "useBatch": false
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.f.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "y99.bps.view",
        "controlId": "fc"
      },
      "routes": [
        {
          "name": "RouteListaBPs",
          "pattern": ":?query:",
          "target": [
            "TargetListaBPs"
          ]
        },
        {
          "name": "RouteParceiro",
          "pattern": "parceiro/{PartnerId}",
          "target": [
            "TargetListaBPs",
            "TargetParceiro"
          ]
        }
      ],
      "targets": {
        "TargetListaBPs": {
          "viewType": "XML",
          "viewId": "ListaBPs",
          "viewName": "ListaBPs",
          "controlAggregation": "beginColumnPages"
        },
        "TargetParceiro": {
          "viewType": "XML",
          "transition": "slide",
          "controlAggregation": "midColumnPages",
          "viewId": "Parceiro",
          "viewName": "Parceiro"
      }
      }
    },
    "rootView": {
      "viewName": "y99.bps.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}
